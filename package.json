{
  "name": "kill-event-sourcing",
  "author": "Gabriel Costa Cerqueira <gabrielcerqueira96@hotmail.com> ",
  "contributors": [
    "Gabriel Costa Cerqueira <gabrielcerqueira96@hotmail.com> "
  ],
  "description": "event sourcing and cqrs toolkit.",
  "version": "1.0.0",
  "keywords": [
    "typescript",
    "event-sourcing",
    "events",
    "cqrs"
  ],
  "bugs": {
    "url": "https://github.com/sptGabriel/kill-event-sourcing/issues",
    "email": "gabrielcerqueira96@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sptGabriel/kill-event-sourcing.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start:dev": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
    "format": "prettier --write --loglevel silent './**/*.{json,js}'",
    "lint": "eslint --ext .ts .",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/.ts": "yarn lint:fix",
    "./**/*.{js,json}": "yarn format"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.3"
  }
}
